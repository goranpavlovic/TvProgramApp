{% extends '::base.html.twig' %}

{% block stylesheets %}
	<link rel="stylesheet" href="{{ asset('bundles/tvdatabasehome/css/stations.css') }}" />
	<link rel="stylesheet" href="{{ asset('bundles/tvdatabasehome/css/catalogue.css') }}" />
{% endblock %}

{% block script %}

{% endblock %}

{% block mainmany %}
	<li><a href="/{{ _locale }}">{% trans %}Home{% endtrans %}</a></li>
	<li><a href="/{{ _locale }}/tvstations">{% trans %}Tv Stations{% endtrans %}</a></li>
	<li><a href="/{{ _locale }}/advancedsearch">{% trans %}Detailed View{% endtrans %}</a></li>
	<li><a href="/{{ _locale }}/search">{% trans %}Search{% endtrans %}</a></li>
	
{% endblock %}

{% block content %} 
	{% for result in results %}
		<div id="entity-type"> {{ result.EntityTypeName }} </div>
			{% for attributeSet in result.AttributeSets %}
				<div id="attribute-set"> {{ attributeSet.AttributeSetName }}</div>
					{% for attribute in attributeSet.attributes %}
						<div id="attribute"> {{ attribute.AttributeName }}</div>
					{% endfor %}
			{% endfor %}
	{% endfor %}
{% endblock %}

  
