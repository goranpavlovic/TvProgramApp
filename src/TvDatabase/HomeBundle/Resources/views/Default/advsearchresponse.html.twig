{% extends '::base.html.twig' %}

{% block stylesheets %}


{% endblock %}

{% block script %}

{% endblock %}

{% block mainmany %}
	<li><a href="/{{ _locale }}">{% trans %}Home{% endtrans %}</a></li>
	<li><a href="/{{ _locale }}/tvstations">{% trans %}Tv Stations{% endtrans %}</a></li>
	<li class="current_page_item" ><a href="/{{ _locale }}/advancedsearch">{% trans %}Advanced Search{% endtrans %}</a></li>
{% endblock %}

{% block content %} 

	<table>
		<tr>
			<th>{% trans %}Televison{% endtrans %}</th>
			<th>{% trans %}Time{% endtrans %}</th>
			<th>{% trans %}Program Name{% endtrans %}</th>
		</tr>
		{% for entity in pagerfanta.currentPageResults %}

			<tr>
				<td>{{ entity.tv.TvName }}</td>
				<td>{{ entity.Datetime|date("m/d/Y") }}</td>
				{%  for attribute in entity.attributes %}
				
					{% if attribute.attributeMeta.AttributeName == "Name" %}
						<td>{{ attribute.Value }}</td>
					{% endif %}
					
				{% endfor %}
			</tr>
			
    		<!--  li>{{ entity.Datetime|date("m/d/Y") }} </li-->

		{% endfor %}
	
	</table>
	<div class="pagerfanta">{{ pagerfanta(pagerfanta, 'default') }}</div>
{% endblock %}


