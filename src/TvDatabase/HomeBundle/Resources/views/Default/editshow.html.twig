{% extends '::base.html.twig' %}

{% block script %}

{% endblock %}

{% block mainmany %}
	<li><a href="/{{ _locale }}">{% trans %}Home{% endtrans %}</a></li>
	<li><a href="/{{ _locale }}/tvstations">{% trans %}Tv Stations{% endtrans %}</a></li>
	<li><a href="/{{ _locale }}/advancedsearch">{% trans %}Detailed View{% endtrans %}</a></li>
	<li><a href="/{{ _locale }}/search">{% trans %}Search{% endtrans %}</a></li>
{% endblock %}

{% block content %}
<form action="/{{ _locale }}/edit/{{ attribute.ValueId }}" method="post">
<table>
	<tr>
		<td>Id:</td>
		<td>{{ attribute.AttributeId }}</td>
	</tr>
	<tr>
		<td>Set:</td>
		<td>{{ attribute.AttributeSetId }}</td>
	</tr>
	<tr>
		<td>Entity:</td>
		<td>{{ attribute.Entity.EntityTypeId }}</td>
	</tr>
	<tr>
		<td>Value:</td>
		<td><input type="text" name="value" value="{{ attribute.value }}"</td>
	</tr>
	<tr>
		<td><input type="submit" value="accept"></td>
	</tr>
</table>
</form>
{% endblock %}

  
